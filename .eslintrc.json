{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "env": {
        "node": true,
        "es2022": true
    },
    "parserOptions": {
        "ecmaVersion": 2022,
        "sourceType": "module"
    },
    "rules": {
        "curly": "error",
        "eqeqeq": "error",
        "no-throw-literal": "error",
        "no-console": "warn",
        "no-debugger": "error",
        "prefer-const": "error",
        "no-var": "error",
        "semi": "error",
        "no-unused-vars": "error"
    },
    "ignorePatterns": [
        "out",
        "dist",
        "**/*.d.ts",
        "webpack.config.js",
        ".vscode-test.mjs"
    ]
}